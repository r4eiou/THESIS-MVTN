{
  "name": "FSL105 Gesture Recognition",
  "dataset": "FSL105",
  "phase": "test",
  "data": {
    "optical_flow": false,
    "type": "rgb",
    "n_classes": 105,
    "n_frames": 40,
    "data_path":  "/datasets/FSL105_ResizedFrames",
    "split_path": "/datasets/FSL105_ResizedFrames/splits", 
    "result_dir": "./result/FSL105",
    "batch_size": 2
  },
  "solver": {
    "workers": 4
  },
  "network": {
    "backbone": "resnet",
    "pretrained": true,
    "ff_size": 1024,
    "n_head": 8,
    "dropout2d": 0.1,
    "dropout1d": 0.5,
    "n_module": 6
  }
}
